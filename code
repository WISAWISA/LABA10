#include <stdio.h> 

int main() 
{ 
char c; 
int words = 0; 
int vowels; 
int cons; 

while((c=getchar())!=EOF) 
{ 
if(c==' '||c=='.'||c==','||c=='\n') 
{ 
if(cons<vowels) 
words++; 

cons = 0; 
vowels = 0; 


} 
else 
{ 
if(c=='a'||c=='e'||c=='i'||c=='o'||c=='u'||c=='y') 
vowels++; 
else 
cons++; 
} 
} 
Printf( “Num of words: %d” , words ); 
Getchar(); 
Return 0; 
}
